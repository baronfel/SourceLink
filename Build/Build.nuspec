<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
  <metadata minClientVersion="2.7">
    <id>SourceLink.Build</id>
    <version>0.0.0</version>
    <authors>Cameron Taggart</authors>
    <description>source linking .NET pdb files with MSBuild</description>
    <projectUrl>http://ctaggart.github.io/SourceLink/</projectUrl>
    <licenseUrl>http://www.apache.org/licenses/LICENSE-2.0</licenseUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <iconUrl>http://ctaggart.github.io/SourceLink/SourceLink128.jpg</iconUrl>
  </metadata>
  <files>
    <file src="*.dll" target="build" />
    <file src="NativeBinaries\amd64\*.dll" target="build\NativeBinaries\amd64" />
    <file src="NativeBinaries\x86\*.dll" target="build\NativeBinaries\x86" />
    <file src="SourceLink.Build.targets" target="build" />
  </files>
</package>