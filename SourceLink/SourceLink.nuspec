<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
  <metadata>
    <id>SourceLink</id>
    <version>0.0.0</version>
    <title>SourceLink</title>
    <authors>Cameron Taggart</authors>
    <!--<summary></summary>-->
    <description>source linking .NET pdb files</description>
    <projectUrl>http://ctaggart.github.io/SourceLink/</projectUrl>
    <licenseUrl>http://www.apache.org/licenses/LICENSE-2.0</licenseUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <iconUrl>http://ctaggart.github.io/SourceLink/SourceLink128.jpg</iconUrl>
    @dependencies@
    <frameworkAssemblies>
      <frameworkAssembly assemblyName="FSharp.Core" />
      <frameworkAssembly assemblyName="Microsoft.Build" />
      <frameworkAssembly assemblyName="System.Xml" />
      <frameworkAssembly assemblyName="System.Configuration" />
    </frameworkAssemblies>
  </metadata>
  <files>
    <file src="SourceLink.dll" target="lib\net45" />
    <file src="SourceLink.pdb" target="lib\net45" />
    <file src="SourceLink.xml" target="lib\net45" />
  </files>
</package>